{
  "name": "pyutils",
  "version": "0.2.5-alpha",
  "description": "Simple basic utils borrowed from Python builtins and libraries.",
  "homepage": "https://github.com/fcole90/pyutils",
  "bugs": {"url" : "https://github.com/fcole90/pyutils/issues"},
  "main": "dist/pyutils.node.js",
  "author": "Fabio Colella",
  "license": "MIT",
  "files": [
    "dist/pyutils.node.js",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type" : "git",
    "url" : "https://github.com/fcole90/pyutils.git"
  },
  "scripts": {
    "unit_test": "mocha --require @babel/register",
    "test": "nyc --reporter=lcov npm run unit_test ",
    "test_strict": "nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 npm run unit_test",
    "test_node_import": "npm node_test.js",
    "coverage": "codecov",
    "build": "rollup -c",
    "watch": "rollup --watch",
    "server": "webpack-dev-server --open"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.5",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "rollup": "^1.31.1",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
