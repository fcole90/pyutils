{
  "name": "basic_utils",
  "version": "0.2.0-alpha",
  "description": "Simple basic utils borrowed from Python builtins and libraries.",
  "main": "./src/basic_utils.js",
  "scripts": {
    "unit_test": "mocha --require @babel/register",
    "test": "nyc --reporter=lcov npm run unit_test ",
    "test_strict": "nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 npm run unit_test",
    "coverage": "codecov",
    "build": "rollup -c",
    "watch": "rollup --watch",
    "server": "webpack-dev-server --open"
  },
  "author": "Fabio Colella",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.5",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "rollup": "^1.31.1",
    "webpack-dev-server": "^3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.4"
  }
}
